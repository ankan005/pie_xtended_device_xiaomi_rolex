#
# system.prop for rolex
#

# DT2W: Enabled (rolex.dt2w=1); Disabled (rolex.dt2w=0)
rolex.dt2w=1

#Lag fixes
debug.hwui.renderer=opengl
debug.hwui.use_buffer_age=false

# USB debugging at boot
persist.sys.usb.config=mtp,adb
ro.adb.secure=0
ro.secure=0
ro.debuggable=1

ro.xtended.version=FirstBlood
ro.xtended.display.version=OFFICIAL
